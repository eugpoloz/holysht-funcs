{
  "comments": false,
  "presets": [
    ["flow"],
    ["es2015"],
    [
      "minify",
      {
        "mangle": {
          "exclude": [
            "appendSmilies",
            "randomUserGenerator",
            "filteredUsers",
            "group_id",
            "howMany",
            "pickAQuite",
            "quotes",
            "joke"
          ]
        }
      }
    ],
    [
      "env",
      {
        "targets": {
          "browsers": [">0.25%", "not ie 11", "not op_mini all"]
        },
        "useBuiltIns": true
      }
    ]
  ]
}
