{
  "comments": false,
  "presets": [
    ["flow"],
    ["es2015"],
    [
      "minify",
      {
        "mangle": {
          "exclude": ["appendSmilies", "randomUserGenerator"]
        }
      }
    ],
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 Chrome versions"]
        },
        "useBuiltIns": true
      }
    ]
  ]
}
